<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="placeHolder" styleClass="pane-with-border" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <VBox prefHeight="50.0" prefWidth="600.0">
      <children>
         <HBox minHeight="50.0" prefWidth="1122.0">
            <padding>
               <Insets bottom="5" left="15" right="5" top="5" />
            </padding>
            <children>
               <VBox prefHeight="200.0" prefWidth="200.0">
                  <children>
                     <Label text="No. of Products:" />
                     <Label fx:id="productQty" text="productQty" />
                  </children>
               </VBox>
               <VBox prefHeight="200.0" prefWidth="200.0">
                  <children>
                     <Label text="Total Profit:" />
                     <Label fx:id="profit" text="profit" />
                  </children>
               </VBox>
               <VBox prefHeight="200.0" prefWidth="200.0">
                  <children>
                     <Label text="Total Revenue:" />
                     <Label fx:id="revenue" text="revenue" />
                  </children>
               </VBox>
            </children>
         </HBox>
         <HBox prefHeight="100.0" prefWidth="200.0" />
      </children>
   </VBox>
</StackPane>
